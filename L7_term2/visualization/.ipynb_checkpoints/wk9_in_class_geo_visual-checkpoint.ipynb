   "source": [
    "from shapely.geometry import Point, Polygon\n",
    "\n",
    "# Create a Point object\n",
    "point = Point(0.5, 0.5)\n",
    "\n",
    "# Create a Polygon object\n",
    "polygon = Polygon([(0, 0), (1, 0), (1, 1), (0, 1)])\n",
    "\n",
    "# Check if the point is within the polygon\n",
    "print(point.within(polygon))\n"
   ]
  }
